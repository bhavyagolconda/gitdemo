pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                cleanWs()
                echo 'Hello World'
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/bhavyagolconda/gitdemo']])
                
            }
        }
        
          stage('Hello') {
            steps {
                echo 'Hello World'
                
            }
        }
    }
    }
